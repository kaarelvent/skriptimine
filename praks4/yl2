#!/bin/bash

#Esitan k端simusi, et leida vajalikud arvud
echo -n "Sisesta aluskandiku pikkus ja laius cm-tes "
read pikkus laius
echo -n "Kui suuri k端psiseid tahad tordile? (cm-tes; pikkus ja laius) "
read pikkus1 laius2
echo -n "Mitu kihti soovid tordile? "
read kihti
echo -n "Mitu k端psist on pakis? "
read pakis

#arvutan pakkide arvu
echo -n "Sul on vaja osta nii palju pakke k端psiseid: "
expr $pikkus \* $laius / $pikkus1 / $laius2 \* $kihti / $pakis 

